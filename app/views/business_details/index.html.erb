<p id="notice"><%= notice %></p>

<h1>Listing Business Details</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Website</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Facebook</th>
      <th>Twitter</th>
      <th>Business</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @business_details.each do |business_detail| %>
      <tr>
        <td><%= business_detail.description %></td>
        <td><%= business_detail.website %></td>
        <td><%= business_detail.phone %></td>
        <td><%= business_detail.email %></td>
        <td><%= business_detail.facebook %></td>
        <td><%= business_detail.twitter %></td>
        <td><%= business_detail.business_id %></td>
        <td><%= link_to 'Show', business_detail %></td>
        <td><%= link_to 'Edit', edit_business_detail_path(business_detail) %></td>
        <td><%= link_to 'Destroy', business_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Business detail', new_business_detail_path %>
